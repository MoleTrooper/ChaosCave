            <kind>1</kind>
            <string>if (!running) exit;

global.avTimer++;

//if player is dead, kill all attacks
if (!instance_exists(objPlayer))
{
    with (objAttack) instance_destroy();
    instance_destroy();
}

if (global.avTimer == 1)
{
    scrStopMusic();
    scrPlayMusic(musBeautifulChaos, false);
    
    with (objPlayer)
    {
        infjump = true;
        maxhp = 3;
        hp = maxhp;
    }
    
    with (objColorCycleController) active = true;
    
    //global.avTimer = 8200; //end portal debug
}
else if (global.avTimer &lt; 80)
{
    with (objText) { x = objPlayer.x; y = objPlayer.y - 48; }
    
    if (global.avTimer &lt; 40) with (objText) alpha += 1/50;
    else with (objText) alpha -= 1/50;
}
else if (global.avTimer == 100) with (objText) instance_destroy();
else if (global.avTimer == 9650) //end it
{
    scrStopMusic();
    instance_create(room_width/2 - 16, room_height - 4*32, objBossWarp);
}

//start and stop attacks
with (objAttack)
{
    if (global.avTimer == startTime)
    {
        running = true;
    }
    else if (global.avTimer == endTime)
    {
        instance_destroy();
    }
}


with (objBigRock) blendColor = global.blendColor;
