            <kind>1</kind>
            <string>if (running)
{
    counter++;
    
    if (!instance_exists(objBigRock)) exit; //make sure the big rock exists so we don't crash
    
    if (counter == 1090) //center burst at the end
    {
        var angle = random(14);
        while (angle &lt; 360)
        {
            var bullet = instance_create(objBigRock.x, objBigRock.y, objStoneBullet);
            bullet.direction = angle;
            bullet.speed = 5;
                
            angle += 15;
        }
    }
    if (counter &gt; 1090) //end slowdown
    {
        if (counter &lt; 1130)
        {
            with (objStutterShot)
            {
                speed *= 0.95;
            }
        }
        else if (counter == 1130)
        {
            with (objStutterShot) speed = 0;
        }
        else if (counter == 1200) //shatter bullets
        {
            var numShots = 3;
            with (objStutterShot)
            {
                //if outside room or too far in the walls, destroy right away, otherwise burst into smaller bullets
                if (x &gt; 16 &amp;&amp; x &lt; (room_width - 16) &amp;&amp; y &gt; 16 &amp;&amp; y &lt; (room_height - 16))
                {
                    var angle = 0;
                    for (var i = 0; i &lt; numShots; i++)
                    {
                        var bullet = instance_create(x, y, objStoneBullet);
                        bullet.image_xscale = 0.5;
                        bullet.image_yscale = 0.5;
                
                        angle += random_range((360 - angle)/((numShots - i)*3), (360 - angle)/(numShots - i));
                        bullet.direction = angle;
                        bullet.speed = 5;
                    }
                }
                instance_destroy();
            }
        }
    }
    else
    {
        //spawn a shot
        if (counter mod shotInterval == 0)
        {
            var bullet = instance_create(objBigRock.x, objBigRock.y, objStutterShot);
            var playerDir = point_direction(objBigRock.x, objBigRock.y, objPlayer.x, objPlayer.y);
            if (counter mod (shotInterval*atPlayerInterval) == 0)
            {
                bullet.direction = random_range(playerDir - playerArea, playerDir + playerArea);
            }
            else bullet.direction = random_range(playerDir + playerArea + clearZone, (playerDir - playerArea - clearZone) + 360);
            
            bullet.speed = bulletSpeed;
        }
        
        //stop and start
        if ((counter / 8.5) % 1 == 0)
        {
            if (moving)
            {
                moving = false;
                with (objStutterShot)
                {
                    if (other.counter &lt; 540) speed = 0;
                    else speed = other.bulletSpeed / 3;
                    curveAmount = curveDir * other.curve;
                    curveDir = -curveDir;
                }
            }
            else
            {
                moving = true;
                with (objStutterShot)
                {
                    speed = other.bulletSpeed;
                    curveAmount = 0;
                }
            }
        }
    }
}
