          <argument>
            <kind>1</kind>
            <string>event_inherited();

if (running)
{
    //rotate stream of rocks
    if (rotation &gt; 90 + rotRange || rotation &lt; 90 - rotRange) rotAmount = -rotAmount;
    rotation += rotAmount;
    
    
    if (counter mod shotInterval == 0) //spawn a rock
    {
        var rock = instance_create(room_width/2, room_height/2, objToFire);
        rock.gravity = gravStrength;
        rock.image_xscale = bulletSize;
        rock.image_yscale = bulletSize;
        rock.direction = rotation + random(rotAmount*shotInterval);
        rock.speed = bulletSpeed;
    }
    
    if (global.avTimer == 4080) //spawn the first laser
    {
        laser1 = instance_create(room_width/2, room_height/2, objLaser);
        
        if (objPlayer.x &gt; room_width/2) laserDir = -1;
        laser1.direction = 90 + laserDir*15;
    }
    else if (global.avTimer == 4120) //start the first laser
    {
        laser1.firing = true;
        laser1.image_yscale = 0;
        laser1.turnAmount = laserDir*200/(4280-4120);
    }
    else if (global.avTimer == 4280) //stop the first laser
    {
        laser1.turnAmount = 0;
        laser1.destroy = true;
    }
    else if (global.avTimer == 4330) // spawn the second laser
    {
        laserDir = -laserDir;
        laser2 = instance_create(room_width/2, room_height/2, objLaser);
        
        laser2.direction = 90 + laserDir*10;
    }
    else if (global.avTimer == 4380) //start the second laser
    {
        laser2.firing = true;
        laser2.image_yscale = 0;
        laser2.turnAmount = laserDir*200/(4530-4380);
    }
    else if (global.avTimer == 4530) //stop the second laser and spawn dual lasers
    {
        laser2.turnAmount = 0;
        laser2.destroy = true;
        
        laser1 = instance_create(room_width/2, room_height/2, objLaser);
        laser2 = instance_create(room_width/2, room_height/2, objLaser);
        laser1.direction = 90 + 15;
        laser2.direction = 90 - 15;
    }
    else if (global.avTimer == 4580) //start dual lasers
    {
        laser1.firing = true;
        laser1.image_yscale = 0;
        laser1.turnAmount = 140/(4675-4580);
        laser2.firing = true;
        laser2.image_yscale = 0;
        laser2.turnAmount = -140/(4675-4580);
    }
    else if (global.avTimer == 4675) //stop dual lasers
    {
        laser1.turnAmount = 0;
        laser2.turnAmount = 0;
    }
    else if (global.avTimer == 4735) //start fast dual laser spin
    {
        var angleDiff = 360 - abs(laser2.direction - laser1.direction);
        laser1.turnAmount = -angleDiff/(4780-4735);
        laser2.turnAmount = angleDiff/(4780-4735);
    }
    else if (global.avTimer == 4770) //remove lasers and start firing cluster bombs
    {
        laser1.destroy = true;
        laser2.destroy = true;
        
        objToFire = objClusterbomb;
        shotInterval = 12;
        bulletSize = 0.8;
    }
}
